{
  "name": "poliorama-backend",
  "version": "0.0.1",
  "description": "backend application for political data visualization",
  "main": "./src/server.ts",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=poliorama:* nodemon",
    "start:production": " pm2 start ecosystem.yaml --env production",
    "build": "NODE_ENV=development tsc",
    "build:production": "NODE_ENV=production tsc",
    "test": "NODE_ENV=test mocha -r ts-node/register ./src/**/*.spec.ts"
  },
  "mocha": {
    "exit": true,
    "reporter": "nyan",
    "full-trace": false
  },
  "nodemonConfig": {
    "ignore": [
      "*.spec.*",
      "*.spec.*"
    ],
    "delay": "2500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afbayonac/poliorama-backend.git"
  },
  "keywords": [
    "data",
    "poli",
    "orama",
    "visual",
    "graph",
    "relationships",
    "colombia"
  ],
  "author": "afbayonac",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/afbayonac/poliorama-backend/issues"
  },
  "homepage": "https://github.com/afbayonac/poliorama-backend#readme",
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/keyv": "^3.1.1",
    "arangojs": "^6.13.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.0",
    "moment": "^2.24.0",
    "oauth-1.0a": "^2.2.6",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.9",
    "@types/cors": "^2.8.6",
    "@types/dirty-chai": "^2.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.10",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/supertest": "^2.0.8",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
